
pipeline {
	agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "Building..\n"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing..\n"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying....\n"'
            }
        }
    }
}
