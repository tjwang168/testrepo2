
pipeline {
	agent any

    stages {
        stage('Build') {
            steps {
		set -x
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
		set -x
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
		set -x
                echo 'Deploying....'
            }
        }
    }
}
